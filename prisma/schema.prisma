generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "mongodb"
}

model Experience {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  company     String
  title       String
  location    String
  startDate   DateTime
  endDate     DateTime
  description String
}

model Projects {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  projectName    String
  startDate      DateTime
  endDate        DateTime
  description    String
  deploymentLink String
  githubLink     String
}


